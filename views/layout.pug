block variables
doctype html
html
	head
		script(src="/scripts/cookie-consent.js")
		script.
			var cookieConsent = new CookieConsent({privacyPolicyUrl: "/privacy-policy.html"});			
		script(async, src="https://www.googletagmanager.com/gtag/js?id=G-DF2C930L0F")
		script(async, src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-1583146712153886", crossorigin="anonymous")
		script(async, src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js", integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p", crossorigin="anonymous")
		script.
			if (cookieConsent.trackingAllowed()) {
				window.dataLayer = window.dataLayer || [];
				function gtag(){dataLayer.push(arguments);}
				gtag('js', new Date());
				gtag('config', 'G-DF2C930L0F');
			}
		meta(charset="utf-8")
		link(async, rel="stylesheet", href="/style.css")
		link(async, rel="stylesheet", href="/cookie-consent.css")
		link(async, href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css", rel="stylesheet", integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3", crossorigin="anonymous")
		link(rel="icon", type="image/png", sizes="32x32", href="/images/icons/favicon-32x32.png")
		link(rel="icon", type="image/png", sizes="16x16", href="/images/icons/favicon-16x16.png")
		link(rel="apple-touch-icon", type="image/png", sizes="180x180", href="/images/icons/apple-touch-icon.png")
		script(src="https://cdn.jsdelivr.net/npm/chart.js")
		script(src="https://cdn.jsdelivr.net/npm/chartjs-plugin-datalabels@2")
		meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
		meta(name="theme-color", content="#000000")
		title #{title} | Fallalytics
		script.
			Chart.register({
				ChartDataLabels
			});
	body
		div#root
			block layout-content